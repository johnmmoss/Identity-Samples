<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="ConsoleTrace"/>
      </listeners>
    </trace>
    <sources>
      <source name="ComponentSpace.SAML2" switchValue="Off">
        <listeners>
          <add name="ConsoleTrace"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="ConsoleTrace" type="System.Diagnostics.ConsoleTraceListener"/>
    </sharedListeners>
  </system.diagnostics>
</configuration>